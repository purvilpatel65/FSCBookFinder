<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.me.fscbookfinder">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon64"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon64"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/ADMOB_APP_ID"/>

        <activity android:name=".SearchActivity" />
        <activity
            android:name=".ListActivity"
            android:parentActivityName=".SearchActivity" />
        <activity
            android:name=".ScanActivity"
            android:parentActivityName=".SearchActivity" />
        <activity
            android:name=".Description_Activity"
            android:parentActivityName=".SearchActivity" />
        <activity
            android:name=".AddActivity"
            android:parentActivityName=".SearchActivity" />

        <activity android:name=".LogInActivity"></activity>
        <activity android:name=".RegisterActivity" />

        <activity
            android:name=".SellingCartActivity"
            android:parentActivityName=".SearchActivity" /><![CDATA[
        />

        ]]>
        <activity
            android:name=".EmailActivity"
            android:parentActivityName=".SearchActivity"/>

        <activity
            android:name=".RateActivity"
            android:parentActivityName=".SearchActivity"/>

        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MyAccountActivity"
            android:parentActivityName=".SearchActivity">

        </activity>
    </application>

</manifest>